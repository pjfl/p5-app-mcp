<header id="header">[% nav.render %]</header>
<aside id="sidebar" class="sidebar [% nav.link_display_class %]"></aside>
<main id="main" class="main">
  <div class="main-container">
    <div id="[% nav.container_name %]" class="[% nav.container_class %]">
      <div id="[% nav.content_name %]" class="[% nav.content_class %]">
[% content %]
      </div>
    </div>
    <footer id="footer">
      <span class="version-number">v[% version %]</span>
    </footer>
  </div>
</main>
