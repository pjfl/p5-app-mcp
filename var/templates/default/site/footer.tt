[% IF config.footer_links;
      '<span></span>';
      FOR link IN config.footer_links;
      IF link.2; href = link.1; ELSE; href = uri_for_action(link.1); END;
      '<a href="' _ href _ '">';
      '<span class="secondary-link">' _ link.0 _ '</span></a>';
      END;
   ELSE;
      '<span></span>';
   END;
%]
